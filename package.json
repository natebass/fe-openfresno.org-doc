{
  "name": "fe-openfresno.org-doc",
  "version": "1.1.0",
  "description": "Storybook documentation for the Open Fresno website.",
  "type": "module",
  "private": false,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "next build",
    "build:storybook": "storybook build",
    "test": "vitest",
    "test:unit": "vitest --project=unit",
    "test:sb": "vitest --project=storybook",
    "test:ui": "vitest --ui",
    "test:debug": "playwright test --project=chromium --headed --debug",
    "storybook": "storybook dev -p 6006",
    "storybook:ci": "storybook dev --no-open -p 6006",
    "storybook:doctor": "storybook doctor --package-manager pnpm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openfresno/fe-openfresno.org-doc.git"
  },
  "keywords": [
    "documentation",
    "nextjs",
    "storybook",
    "tailwind",
    "playwright",
    "vitest",
    "meetup",
    "civic",
    "nextjs",
    "california",
    "fresno"
  ],
  "author": {
    "name": "Open Fresno",
    "email": "hello@openfresno.org",
    "url": "https://openfresno.org"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/openfresno/fe-openfresno.org-doc/issues"
  },
  "homepage": "https://github.com/openfresno/fe-openfresno.org-doc#readme",
  "dependencies": {
    "@mdx-js/loader": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "@next/mdx": "^16.1.6",
    "@types/mdx": "^2.0.13",
    "keen-slider": "^6.8.6",
    "moment": "^2.30.1",
    "next": "^16.1.6",
    "next-mdx-remote": "^5.0.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "swr": "^2.3.8",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.0",
    "@storybook/addon-a11y": "^10.2.1",
    "@storybook/addon-docs": "^10.2.1",
    "@storybook/addon-vitest": "^10.2.1",
    "@storybook/nextjs-vite": "^10.2.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
